{
  "singleQuote": true,
  "arrowParens": "avoid",
  "trailingComma": "all",
  "arrow-parens": "always",
  "importOrder": [
    "(.*)react(.*)",
    "(.*)redux(.*)\",",
    "<THIRD_PARTY_MODULES>",
    "^~[^/UIKit](.*)$",
    "^~/router(.*)$",
    "^~/assets(.*)$",
    "^~/UIKit(.*)$",
    "^~/css(.*)$",
    "^[./]"
  ],
  "importOrderSortSpecifiers": true,
  "order": "smacss",
  "keepOverrides": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrderSeparation": true
}
