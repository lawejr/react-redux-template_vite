{
  "name": "react-redux-template_vite",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "Project template with ts/react/redux/styled-components stack",
  "scripts": {
    "start": "vite --port 3000 --open",
    "build": "tsc && vite build",
    "preview": "npm run build && vite preview",
    "precommit-lint": "tsc --noEmit && lint-staged",
    "server": "json-server --watch src/examples/db.json --port 1111",
    "analyze": "vite-bundle-visualizer",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@reduxjs/toolkit": "^2.5.1",
    "@sentry/react": "^9.1.0",
    "@sentry/tracing": "^7.120.3",
    "@sentry/types": "^9.1.0",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "dayjs": "^1.11.13",
    "final-form": "^4.20.10",
    "immer": "^10.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-final-form": "^6.5.9",
    "react-redux": "^9.2.0",
    "react-responsive": "^10.0.0",
    "react-router-dom": "^7.1.5",
    "serializr": "^3.0.3",
    "use-immer": "^0.11.0"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.13.5",
    "@eslint/compat": "^1.2.6",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.20.0",
    "@nabla/vite-plugin-eslint": "^2.0.5",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.20.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^3.8.1",
    "husky": "^9.1.7",
    "json-server": "^1.0.0-beta.3",
    "lint-staged": "^15.4.3",
    "postcss": "^8.5.2",
    "prettier": "^3.5.1",
    "prettier-plugin-css-order": "^2.1.2",
    "sharp": "^0.33.5",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-favicons-inject": "^2.2.0",
    "vite-plugin-imagemin": "^0.6.1"
  },
  "lint-staged": {
    "*.{js,ts,jxs,tsx},!vite.config.ts": "eslint --max-warnings=0"
  }
}
