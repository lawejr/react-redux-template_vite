{
  "name": "react-redux-template_vite",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "Project template with ts/react/redux/styled-components stack",
  "scripts": {
    "start": "vite --port 3000 --open",
    "build": "tsc && vite build",
    "preview": "npm run build && vite preview",
    "precommit-lint": "tsc --noEmit && lint-staged",
    "server": "json-server --watch src/examples/db.json --port 1111",
    "analyze": "vite-bundle-visualizer",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@reduxjs/toolkit": "^1.9.1",
    "@sentry/react": "^7.27.0",
    "@sentry/tracing": "^7.27.0",
    "@sentry/types": "^7.29.0",
    "final-form": "^4.20.7",
    "immer": "^10.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-final-form": "^6.5.9",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.5.0",
    "serializr": "^3.0.2",
    "use-immer": "^0.9.0"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.10.5",
    "@nabla/vite-plugin-eslint": "^1.5.0",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^4.0.0",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "eslint": "^8.31.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^3.5.2",
    "husky": "^8.0.0",
    "json-server": "^0.17.1",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.2",
    "prettier-plugin-css-order": "^1.3.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-bundle-visualizer": "^0.7.0",
    "vite-plugin-imagemin": "^0.6.1"
  },
  "lint-staged": {
    "*.{js,ts,jxs,tsx},!vite.config.ts": "eslint --max-warnings=0"
  }
}
