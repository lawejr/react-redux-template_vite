{
  "name": "react-redux-template_vite",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "Project template with ts/react/redux/styled-components stack",
  "scripts": {
    "start": "vite --port 3000 --open",
    "build": "tsc && vite build",
    "preview": "npm run build && vite preview",
    "precommit-lint": "tsc --noEmit && lint-staged",
    "server": "json-server --watch src/examples/db.json --port 1111",
    "analyze": "vite-bundle-visualizer",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@reduxjs/toolkit": "^2.2.1",
    "@sentry/react": "^7.104.0",
    "@sentry/tracing": "^7.104.0",
    "@sentry/types": "^7.104.0",
    "final-form": "^4.20.10",
    "immer": "^10.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-final-form": "^6.5.9",
    "react-redux": "^9.1.0",
    "react-responsive": "^9.0.2",
    "react-router-dom": "^6.22.2",
    "serializr": "^3.0.2",
    "use-immer": "^0.9.0"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.11.0",
    "@nabla/vite-plugin-eslint": "^2.0.2",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "husky": "^9.0.11",
    "json-server": "^0.17.4",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-css-order": "^2.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-bundle-visualizer": "^1.0.1",
    "vite-plugin-imagemin": "^0.6.1"
  },
  "lint-staged": {
    "*.{js,ts,jxs,tsx},!vite.config.ts": "eslint --max-warnings=0"
  }
}
