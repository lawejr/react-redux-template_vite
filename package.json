{
  "name": "react-redux-template_vite",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "Project template with ts/react/redux/styled-components stack",
  "scripts": {
    "start": "vite --port 3000 --open",
    "build": "tsc && vite build",
    "preview": "npm run build && vite preview",
    "precommit-lint": "tsc --noEmit && lint-staged",
    "server": "json-server --watch src/examples/db.json --port 1111",
    "analyze": "vite-bundle-visualizer",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@reduxjs/toolkit": "^2.8.2",
    "@sentry/react": "^10.0.0",
    "@sentry/tracing": "^7.120.4",
    "@sentry/types": "^7.120.4",
    "dayjs": "^1.11.13",
    "immer": "^10.1.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-redux": "^9.2.0",
    "react-responsive": "^10.0.1",
    "react-router-dom": "^7.7.1",
    "serializr": "^3.0.5",
    "use-immer": "^0.11.0"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.13.5",
    "@nabla/vite-plugin-eslint": "^2.0.6",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^20.19.9",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "husky": "^9.1.7",
    "json-server": "^0.17.4",
    "lint-staged": "^16.1.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-css-order": "^2.1.2",
    "sharp": "^0.34.3",
    "typescript": "^5.9.2",
    "vite": "^7.0.6",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-favicons-inject": "^2.2.0",
    "vite-plugin-imagemin": "^0.6.1"
  },
  "lint-staged": {
    "*.{js,ts,jxs,tsx},!vite.config.ts": "eslint --max-warnings=0"
  }
}
